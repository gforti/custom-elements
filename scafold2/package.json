{
  "name": "",
  "description": "Development Work",
  "version": "0.0.0",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "scripts": {
    "lint:js": "./node_modules/.bin/eslint --ext .js ./src",
    "lint:css": "./node_modules/.bin/stylelint ./src/**/*.css",
    "lint": "npm run lint:css && npm run lint:js",
    "build:cache": "node build.js",
    "build:release": "npm run build:cache && webpack --mode production --config webpack.config.js",
    "build:beta": "npm run build:cache && webpack --mode development --config webpack.config.js",
    "start": "./node_modules/.bin/nodemon build.js & ./node_modules/.bin/webpack-dev-server --mode development --config webpack.config.js",
    "serve": "npm run build:release && node dist/serve.js",
    "serve:beta": "npm run build:beta && opn http://localhost:9000 && node dist/serve.js",
    "test": "./node_modules/.bin/karma start karma.conf.js",
    "test:container": "./node_modules/karma/bin/karma start karma.conf.js --browsers ChromeCustom"
  },
  "nodemonConfig": {
    "watch": [
      "src/"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "htm,html"
  },
  "eslintIgnore": [
    "build.js"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "arrow-parens": [
        2,
        "as-needed"
      ],
      "curly": 2,
      "indent": [
        2,
        2
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ],
      "no-inline-comments": 0,
      "no-trailing-spaces": 2,
      "no-multiple-empty-lines": [
        2,
        {
          "max": 2,
          "maxEOF": 1
        }
      ],
      "id-blacklist": [
        2,
        "data",
        "err",
        "e",
        "cb",
        "callback",
        "item",
        "items"
      ],
      "func-style": [
        2,
        "declaration",
        {
          "allowArrowFunctions": true
        }
      ],
      "sort-keys": 2,
      "space-in-parens": [
        2,
        "never"
      ],
      "space-before-blocks": 2,
      "sort-vars": 2,
      "sort-imports": 2,
      "quote-props": [
        2,
        "consistent-as-needed"
      ],
      "prefer-object-spread": 2,
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "camelcase": [
        2,
        {
          "allow": [
            "filter_path",
            "movement_status"
          ]
        }
      ],
      "no-unneeded-ternary": 2
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "color-hex-case": "lower",
      "indentation": null,
      "no-empty-source": null,
      "selector-type-no-unknown": [
        true,
        {
          "ignore": [
            "custom-elements",
            "default-namespace"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.10.0",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "is-docker": "1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^3.1.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.8",
    "opn-cli": "^3.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "svg-url-loader": "^2.3.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {}
}
